{
  "dependencies": {
    "@create-figma-plugin/ui": "^4.0.1",
    "@create-figma-plugin/utilities": "^4.0.1",
    "preact": ">=10"
  },
  "devDependencies": {
    "@create-figma-plugin/build": "^4.0.1",
    "@create-figma-plugin/tsconfig": "^4.0.1",
    "@figma/plugin-typings": "1.109.0",
    "autoprefixer": "^10.4.21",
    "concurrently": ">=8",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "typescript": ">=5"
  },
  "scripts": {
  "build:css": "tailwindcss -i ./src/input.css -o ./src/output.css",
  "watch": "npm run build:css && npm run watch:css",
  "watch:css": "tailwindcss -i ./src/input.css -o ./src/output.css --watch"
},
  "figma-plugin": {
    "editorType": [
      "figma"
    ],
    "id": "preact-tailwindcss",
    "name": "Preact Tailwind Css",
    "main": "src/main.ts",
    "ui": "src/ui.tsx"
  }
}
